
@{
    ViewBag.Title = "Home Page";
    Layout = "_Layout";
 }

@section Scripts{
    <script src="~/lib/underscore/underscore-min.js"></script>
    <script type="text/javascript" src="~/lib/angular/angular.min.js"></script>
    <script type="text/javascript" src="~/lib/angular-route/angular-route.min.js"></script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDpAHeZUicX4vRTqOILVuSL2dmswp9WLMo"
        type="text/javascript"></script>
    <script src="~/lib/gmaps/gmaps.min.js"></script>
    <script src="~/lib/travelmap/travelmap.min.js"></script>
    <environment names="Development">
        <script src="~/js/simpleControls.js"></script>
        <script type="text/javascript" src="~/js/app-trip.js"></script>
        <script type="text/javascript" src="~/js/tripsController.js"></script>
        <script type="text/javascript" src="~/js/tripEditorController.js"></script>
    </environment>
    <environment names="Production, Staging">
    <script src="~/lib/_app/simpleControls.js"></script>
    <script type="text/javascript" src="~/lib/_app/app-trip.js.js"></script>
    <script type="text/javascript" src="~/lib/_app/tripsController.js"></script>
    <script type="text/javascript" src="~/lib/_app/tripEditorController.js"></script>
</environment>
}
<div class="row" ng-app="app-trips">
    <div ng-view></div>

</div>